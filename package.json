{
  "name": "@stephenvector/prefab",
  "version": "0.0.1",
  "description": "A set of themeable UI components for React projects.",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "tsc",
    "docz": "docz dev",
    "test": "jest"
  },
  "files": [
    "docs/",
    "doczrc.js",
    "tsconfig.json",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephenvector/prefab.git"
  },
  "author": "Stephen Vector",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stephenvector/prefab/issues"
  },
  "homepage": "https://github.com/stephenvector/prefab#readme",
  "dependencies": {
    "aphrodite": "^2.4.0",
    "docz": "^2.0.0-rc.49"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.0",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "html-webpack-template": "^6.2.0",
    "jest": "^24.9.0",
    "next": "^9.0.6",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-live": "^2.2.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx",
      "node"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.docz/"
    ]
  }
}
