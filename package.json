{
  "name": "@stephenvector/prefab",
  "version": "0.1.0",
  "description": "A set of themeable UI components for React projects.",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "cleanup": "rimraf coverage lib node_modules",
    "storybook": "start-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephenvector/prefab.git"
  },
  "author": "Stephen Vector",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stephenvector/prefab/issues"
  },
  "homepage": "https://github.com/stephenvector/prefab#readme",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@storybook/addon-info": "^5.2.1",
    "@storybook/preset-typescript": "^1.1.0",
    "@storybook/react": "^5.2.1",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.0",
    "@types/storybook__react": "^4.0.2",
    "aphrodite": "^2.4.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.9.0",
    "next": "^9.0.6",
    "react": "^16.10.1",
    "react-docgen-typescript-loader": "^3.3.0",
    "react-dom": "^16.10.1",
    "react-live": "^2.2.0",
    "rimraf": "^3.0.0",
    "storybook-addon-props": "^3.0.4",
    "storybook-addon-react-docgen": "^1.2.28",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.3"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx",
      "node"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
